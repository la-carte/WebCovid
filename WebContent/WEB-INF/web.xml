<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>WebCovid</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>accueil.jsp</welcome-file>
		<welcome-file>connexion.jsp</welcome-file>
		<welcome-file>admin.jsp</welcome-file>
		<welcome-file>user.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>InscriptionServlet</servlet-name>
		<servlet-class>ServletPackage.InscriptionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ConnexionServlet</servlet-name>
		<servlet-class>ServletPackage.ConnexionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BeanServlet</servlet-name>
		<servlet-class>ServletPackage.BeanServlet</servlet-class>
	</servlet>
	
</web-app>